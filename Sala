public class Sala {
    
    //atributos da classe
    String nome;
    String local;
    int capacidade;
    String observacoes;

    //construtor da classe
    public Sala(String nome, String local, int capacidade, String observacoes){
        this.nome = nome;
        this.local = local;
        this.capacidade = capacidade;
        this.observacoes = observacoes;
    }
}
